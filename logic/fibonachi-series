import java.util.*;
import java.io.*;

public class Main {
  public static void main(String[] args) {

int n = 3;
      int i = 0;
      int a = 0;
      int b = 1;
      int c;
      while(i<n){
          c = a +b;
          a = b;
          b = c;
          i++;
      }
      System.out.print(a);
  }
}
